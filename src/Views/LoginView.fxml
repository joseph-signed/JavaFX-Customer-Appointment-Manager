<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="281.0" style="-fx-background-color: blue;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <VBox alignment="CENTER" layoutX="7.0" layoutY="13.0" prefHeight="193.0" prefWidth="268.0" style="-fx-background-color: blue;">
      <Label fx:id="Label_UserTimeZone" />
      <Label fx:id="Label_Appointment" prefHeight="36.0" prefWidth="91.0" text="Log In" textFill="WHITE">
         <font>
            <Font size="25.0" />
         </font></Label>
      <Label fx:id="Label_ErrorMessage" textAlignment="CENTER" textFill="#801919">
         <VBox.margin>
            <Insets bottom="5.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="Label_Username" text="Username" textFill="WHITE">
         <padding>
            <Insets right="20.0" />
         </padding>
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="TextField_Username" prefHeight="7.0" prefWidth="149.0" />
      <Label fx:id="Label_Password" text="Password" textFill="WHITE">
         <padding>
            <Insets right="20.0" />
         </padding>
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="TextField_Password" />
   </VBox>
   <Button fx:id="Button_LogIn" layoutX="93.0" layoutY="229.0" mnemonicParsing="false" onAction="#PressLogIn" text="Log In">
      <padding>
         <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
      </padding>
      <font>
         <Font size="20.0" />
      </font>
   </Button>
</AnchorPane>
