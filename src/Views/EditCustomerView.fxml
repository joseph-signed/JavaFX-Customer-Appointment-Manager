<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="400.0" prefWidth="270.0" style="-fx-background-color: blue;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.EditCustomerController">
   <VBox layoutX="32.0" layoutY="14.0" prefHeight="344.0" prefWidth="205.0">
      <Label prefHeight="17.0" prefWidth="71.0" text="Customer ID" textFill="WHITE" />
      <TextField fx:id="TextBox_CustomerId" disable="true" editable="false" prefHeight="25.0" prefWidth="197.0" />
      <Label prefHeight="17.0" prefWidth="71.0" text="Country" textFill="WHITE" />
      <ComboBox fx:id="ComboBox_CustomerCountry" prefHeight="25.0" prefWidth="196.0" />
      <Label prefHeight="17.0" prefWidth="71.0" text="Division" textFill="WHITE" />
      <ComboBox fx:id="ComboBox_CustomerDivision" prefHeight="25.0" prefWidth="196.0" />
      <Label prefHeight="17.0" prefWidth="71.0" text="Name" textFill="WHITE" />
      <TextField fx:id="TextBox_CustomerName" prefHeight="25.0" prefWidth="227.0" />
      <Label prefHeight="17.0" prefWidth="71.0" text="Address" textFill="WHITE" />
      <TextField fx:id="TextBox_CustomerAddress" prefHeight="25.0" prefWidth="197.0" />
      <Label prefHeight="17.0" prefWidth="71.0" text="Postal Code" textFill="WHITE" />
      <TextField fx:id="TextBox_CustomerPostalCode" prefHeight="25.0" prefWidth="197.0" />
      <Label prefHeight="17.0" prefWidth="133.0" text="Phone Number" textFill="WHITE" />
      <TextField fx:id="TextBox_CustomerPhoneNumber" prefHeight="25.0" prefWidth="197.0" />
      <Button fx:id="Button_Save" mnemonicParsing="false" onAction="#PressSave" prefHeight="25.0" prefWidth="70.0" text="Save" />
      <Button fx:id="Button_Back" mnemonicParsing="false" onAction="#PressBack" prefHeight="25.0" prefWidth="70.0" text="Back" />
   </VBox>
</AnchorPane>
